<div class="page-header">
  <h1>
    My Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Caption</th>
        <th>Image</th>
      </tr>

      <% @likes.each do |like| %>
      <tr>
        <td> <%= like.photo.caption %></td>
        <td><img src="<%= like.photo.image %>" width="400"></td>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
